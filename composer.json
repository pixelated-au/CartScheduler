{
    "name": "pixelated-au/cart-scheduler",
    "type": "project",
    "description": "Cart Scheduler project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3.0",
        "ext-pdo": "*",
        "ext-zip": "*",
        "bugsnag/bugsnag-laravel": "^2.28.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/jetstream": "^5.1.4",
        "laravel/sanctum": "^4.0.2",
        "laravel/tinker": "^2.9.0",
        "maatwebsite/excel": "^3.1.67",
        "maennchen/zipstream-php": "^3.1",
        "pixelated-au/streamline": "dev-main",
        "psr/simple-cache": "^3.0.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-data": "^4.17",
        "spatie/laravel-ray": "^1.36.2",
        "spatie/laravel-settings": "^3.3.2",
        "spatie/laravel-tags": "^4.6.1",
        "spatie/laravel-typescript-transformer": "^2.5",
        "tightenco/ziggy": "^2.3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "brianium/paratest": "^7.8.1",
        "dg/bypass-finals": "^1.8.0",
        "driftingly/rector-laravel": "^2.0.7",
        "ergebnis/phpunit-slow-test-detector": "^2.19",
        "fakerphp/faker": "^1.23.1",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.17.2",
        "laravel/sail": "^1.31.1",
        "league/csv": "^9.16",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.4.0",
        "phpunit/phpunit": "^11.3.6",
        "rector/rector": "^2.1.7",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Factories\\Sequences\\": "database/factories/sequences",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "ziggy": "@php artisan ziggy:generate --types",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php composer audit",
            "which npm > /dev/null && npm audit || echo 'npm not found, skipping npm audit'",
            "# *** DON'T FORGET TO RUN: snyk test ***"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "prefer-stable": true
}
